  inherited qryConsultaRelatorio: TSQLQuery
    Active = True
    MaxBlobSize = -1
    SQL.Strings = (
      'SELECT'
      '    P.PES_TIPOSANG AS '#39'Tipo Sangu'#237'neo'#39','
      '    COUNT(D.DOA_ID) AS '#39'Quantidade'#39
      'FROM'
      '    BS_DOACAO D'
      'INNER JOIN'
      '    BS_PESSOA P ON D.PES_ID = P.PES_ID'
      'GROUP BY'
      '    P.PES_TIPOSANG'
      'ORDER BY'
      '    COUNT(D.DOA_ID) DESC;')
    SQLConnection = dmConexao.conConexao
